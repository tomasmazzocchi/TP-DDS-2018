{{#partial "contenido"}}
    <meta charset="UTF-8">

    <div style="height:20%; padding:15px; height:auto" class="card evaluacionIndicador col l4  m6 s12 offset-l4 offset-m3">
       <div style="text-align:center;height:46px" class="input-field col l6 m6 s12">
         <select>
         	 {{#each indicadores}}
         	 {{#select indicador}} 
             <option value = "nombre">{{this.nombre}}</option>
             {{/select }}
             {{/each}}
         </select>
       </div>
       <div style="text-align:center;padding-top:25px;" class="col l6 m6 s12">
         <a class="waves-effect waves-light btn blue">Calcular Indicador</a>
       </div>
       <div class="datos col l6 m6 s12">
         <span class="title col s12">Formula</span>
         <span class="dato col s12">{{this.formula}}</span>
       </div>
       <div class="datos col l6 m6 s12">
         <span class="title col s12">Resultaado</span>
         <span class="dato col s12">200</span>
       </div>
    </div>



     <script type="text/javascript">
       $(document).ready(function() {
           $('select').material_select();
       });
     </script>

  {{/partial}}

{{>index.hbs}}
